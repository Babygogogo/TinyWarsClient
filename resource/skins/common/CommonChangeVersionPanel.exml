<?xml version="1.0" encoding="utf-8"?>
<e:Skin width="960" height="540" xmlns:e="http://ns.egret.com/eui" xmlns:GameUi="TinyWars.GameUi.*"
        xmlns:w="http://ns.egret.com/wing">
    <GameUi:UiImage id="_imgMask" source="commonColorTransparent0001" left="0" right="0" top="0" bottom="0"
                    scale9Grid="1,1,2,2" />
    <e:Group id="_group" width="640" horizontalCenter="0" height="360" verticalCenter="0">
        <GameUi:UiImage anchorOffsetX="0" anchorOffsetY="0" left="0" right="0" top="0" bottom="0"
                        source="commonColorBlack0001" scale9Grid="1,1,2,2" locked="true" />
        <GameUi:UiLabel id="_labelTitle" text="Change Version" y="12" size="24" bold="true" horizontalCenter="0" />
        <GameUi:UiScrollList id="_listVersion" horizontalCenter="0" height="200" y="50" width="560">
            <e:List>
                <e:itemRendererSkinName>
                    <e:Skin width="560" height="80" currentState="up" states="up,down">
                        <GameUi:UiImage source="commonPentagon0002" left="0" right="0" top="0" bottom="0"
                                        visible.down="false" scale9Grid="4,4,1,1" />
                        <GameUi:UiImage left="0" right="0" top="0" bottom="0" x="10" y="10" source="commonPentagon0003"
                                        visible.up="false" scale9Grid="4,4,1,1" />
                        <GameUi:UiButton id="_btnChoose" label="" left="0" right="0" top="0" bottom="0"
                                         visible.down="false">
                            <GameUi:skinName>
                                <e:Skin states="up,down,disabled">
                                    <e:Image source="commonColorTransparent0000" left="0" right="0" top="0" bottom="0" />
                                    <e:Label id="labelDisplay" horizontalCenter="0" verticalCenter="0" size="22"
                                             bold="true" textColor="0x000000" />
                                </e:Skin>
                            </GameUi:skinName>
                        </GameUi:UiButton>
                        <GameUi:UiLabel id="_labelName" text="地图名称" size="18" x="10" y="10" bold="true" />
                        <GameUi:UiLabel id="_labelCurrent" text="地图名称" size="18" y="10" bold="true" right="10" />
                        <GameUi:UiLabel id="_labelDesc" text="地图名称" size="14" y="34" left="10" right="10" />
                    </e:Skin>
                </e:itemRendererSkinName>
                <e:layout>
                    <e:VerticalLayout/>
                </e:layout>
            </e:List>
        </GameUi:UiScrollList>
        <GameUi:UiLabel id="_labelTips" text="Change Version" y="260" size="14" right="40" left="40" wordWrap="true"
                        textAlign="center" />
        <e:Group horizontalCenter="0" bottom="20">
            <GameUi:UiButton id="_btnBack" label="Back" y="0" height="30" width="120"
                             skinName="Skins.Component.Button.ButtonBlue001" x="0" />
            <GameUi:UiButton id="_btnConfirm" label="下一步" height="30" width="120"
                             skinName="Skins.Component.Button.ButtonYellow001" x="20" y="470" />
            <e:layout>
                <e:HorizontalLayout gap="60" />
            </e:layout>
        </e:Group>
    </e:Group>
</e:Skin>
