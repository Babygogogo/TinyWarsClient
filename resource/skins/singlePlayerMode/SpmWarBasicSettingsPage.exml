<?xml version="1.0" encoding="utf-8"?>
<e:Skin xmlns:e="http://ns.egret.com/eui" xmlns:GameUi="TinyWars.GameUi.*" xmlns:w="http://ns.egret.com/wing">
    <e:Group left="0" right="0" top="0" bottom="0">
        <e:Scroller left="0" right="20" bottom="20" top="0">
            <e:Group>
                <e:Group x="0" y="0" height="40" width="420" locked="true">
                    <GameUi:UiImage left="0" right="0" top="0" bottom="0" source="commonColorBlack0000" alpha="0.4"
                                    scale9Grid="1,1,2,2" />
                    <GameUi:UiLabel id="_labelMapNameTitle" text="Map Name" size="16" maxTextWidth="100" x="10"
                                    verticalCenter="0" bold="true" textColor="0xFFFFFF" />
                    <e:Group verticalCenter="0" height="24" right="6" width="270">
                        <GameUi:UiImage source="commonRectangle0001" scale9Grid="2,2,1,1" left="0" right="0" top="0"
                                        bottom="0" />
                        <GameUi:UiLabel id="_labelMapName" size="16" touchEnabled="false" text="Yes" x="10"
                                        verticalCenter="0" textColor="0xFFFFFF" />
                    </e:Group>
                </e:Group>
                <e:Group x="10" y="10" height="40" width="420">
                    <GameUi:UiImage left="0" right="0" top="0" bottom="0" source="commonColorBlack0000" alpha="0.4"
                                    scale9Grid="1,1,2,2" />
                    <GameUi:UiLabel id="_labelSaveSlotTitle" text="Save Slot" size="16" maxTextWidth="100" x="10"
                                    verticalCenter="0" bold="true" textColor="0xFFFFFF" />
                    <e:Group verticalCenter="0" height="24" right="6" width="270">
                        <GameUi:UiImage source="commonRectangle0001" scale9Grid="2,2,1,1" left="0" right="0" top="0"
                                        bottom="0" />
                        <GameUi:UiLabel id="_labelSaveSlot" size="16" touchEnabled="false" text="Yes" x="10"
                                        verticalCenter="0" textColor="0xFFFFFF" />
                    </e:Group>
                </e:Group>
                <e:Group x="20" y="20" height="40" width="420">
                    <GameUi:UiImage left="0" right="0" top="0" bottom="0" source="commonColorBlack0000" alpha="0.4"
                                    scale9Grid="1,1,2,2" />
                    <GameUi:UiLabel id="_labelSlotCommentTitle" text="Slot Comment" size="16" maxTextWidth="100" x="10"
                                    verticalCenter="0" bold="true" textColor="0xFFFFFF" />
                    <e:Group verticalCenter="0" height="24" right="6" width="270">
                        <GameUi:UiImage source="commonRectangle0001" scale9Grid="2,2,1,1" left="0" right="0" top="0"
                                        bottom="0" />
                        <GameUi:UiLabel id="_labelSlotComment" size="16" touchEnabled="false" text="Yes" x="10"
                                        verticalCenter="0" textColor="0xFFFFFF" />
                    </e:Group>
                </e:Group>
                <e:Group x="0" y="0" height="40" width="420">
                    <GameUi:UiImage left="0" right="0" top="0" bottom="0" source="commonColorBlack0000" alpha="0.4"
                                    scale9Grid="1,1,2,2" />
                    <GameUi:UiLabel id="_labelWarRuleTitle" text="War Rule" textColor="0xFFFFFF" size="16"
                                    maxTextWidth="100" x="10" verticalCenter="0" bold="true" />
                    <e:Group verticalCenter="0" height="24" right="6" width="270">
                        <GameUi:UiImage source="commonRectangle0001" scale9Grid="2,2,1,1" left="0" right="0" top="0"
                                        bottom="0" />
                        <GameUi:UiLabel id="_labelWarRule" size="16" touchEnabled="false" text="Yes" x="10"
                                        verticalCenter="1" textColor="0xFFFFFF" />
                    </e:Group>
                </e:Group>
                <e:Group x="0" y="-36" height="40" width="420">
                    <GameUi:UiImage left="0" right="0" top="0" bottom="0" source="commonColorBlack0000" alpha="0.4"
                                    scale9Grid="1,1,2,2" />
                    <GameUi:UiLabel id="_labelHasFogTitle" text="FoW" textColor="0xFFFFFF" size="16" maxTextWidth="100"
                                    x="10" verticalCenter="0" bold="true" />
                    <GameUi:UiButton id="_btnHasFogHelp" x="120" verticalCenter="0">
                        <GameUi:skinName>
                            <e:Skin>
                                <e:Group horizontalCenter="0" verticalCenter="0">
                                    <e:Image source="commonIcon0001" />
                                    <e:Label id="labelDisplay" horizontalCenter="0" verticalCenter="0" size="25"
                                             bold="true" textColor="0xffffff" />
                                    <GameUi:UiImage id="_imgExtra" verticalCenter="0" horizontalCenter="0" />
                                </e:Group>
                            </e:Skin>
                        </GameUi:skinName>
                    </GameUi:UiButton>
                    <e:Group verticalCenter="0" height="24" right="6" width="270">
                        <GameUi:UiImage source="commonRectangle0001" scale9Grid="2,2,1,1" left="0" right="0" top="0"
                                        bottom="0" />
                        <GameUi:UiLabel id="_labelHasFog" size="16" touchEnabled="false" text="Yes" x="10"
                                        verticalCenter="1" textColor="0xFFFFFF" />
                    </e:Group>
                </e:Group>
                <e:layout>
                    <e:VerticalLayout />
                </e:layout>
            </e:Group>
        </e:Scroller>
    </e:Group>
</e:Skin>
