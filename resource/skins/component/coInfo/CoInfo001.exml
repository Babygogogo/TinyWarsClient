                <TwnsUiLabel:UiLabel id="_labelDesigner" text="???" x="81" y="69" size="14" />
                <e:layout>
                    <e:VerticalLayout horizontalAlign="right" gap="2" />
                </e:layout>
            </e:Group>
        </e:Group>
