                        <e:layout>
                            <e:HorizontalLayout/>
                        </e:layout>
                    </e:Group>
                    <TwnsUiLabel:UiLabel id="_labelMapName" text="地图名" y="0" size="18" x="0" bold="true" />
                    <e:layout>
                        <e:VerticalLayout gap="2" />
                    </e:layout>
                </e:Group>
                <e:Group x="0" y="52">
                    <e:Group x="0">
                        <TwnsUiLabel:UiLabel id="_labelPlayedTimesTitle" text="全服游戏次数" y="10" size="12" x="10" />
                        <TwnsUiLabel:UiLabel id="_labelPlayedTimes" text="100" y="0" size="18" x="0"
                                             textColor="0xDC6868" bold="true" />
                        <e:layout>
                            <e:VerticalLayout gap="2" />
                        </e:layout>
                    </e:Group>
                    <e:Group x="80">
                        <TwnsUiLabel:UiLabel id="_labelRatingTitle" text="评分" y="10" size="12" x="10" />
                        <TwnsUiLabel:UiLabel id="_labelRating" text="100" y="0" size="18" x="0" bold="true"
                                             textColor="0x68DC68" />
                        <e:layout>
                            <e:VerticalLayout gap="2" />
                        </e:layout>
                    </e:Group>
                </e:Group>
                <e:Group id="_groupMyRating" x="80" y="85">
                    <e:Group x="0" scaleX="1" scaleY="1">
                        <TwnsUiLabel:UiLabel id="_labelMyRatingTitle" text="评分" y="10" size="12" x="10" />
                        <TwnsUiLabel:UiLabel id="_labelMyRating" text="100" y="0" size="18" x="0" bold="true"
                                             textColor="0x68DC68" />
                        <e:layout>
                            <e:VerticalLayout gap="2" />
                        </e:layout>
                    </e:Group>
                    <TwnsUiImage:UiImage id="_imgSetMyRating" source="lobbyTop0001" width="20" height="20" x="47" y="13" />
                    <e:layout>
                        <e:HorizontalLayout verticalAlign="middle" />
                    </e:layout>
                </e:Group>
                <e:Group x="0" y="104">
                    <e:Group x="0">
                        <TwnsUiLabel:UiLabel id="_labelPlayersCountTitle" text="玩家数" y="10" size="12" x="10" />
                        <TwnsUiLabel:UiLabel id="_labelPlayersCount" text="100" y="0" size="18" x="0" bold="true" />
                        <e:layout>
                            <e:VerticalLayout gap="2" />
                        </e:layout>
                    </e:Group>
                    <e:Group x="80">
                        <TwnsUiLabel:UiLabel id="_labelMapSizeTitle" text="地图尺寸" y="10" size="12" x="10" />
                        <TwnsUiLabel:UiLabel id="_labelMapSize" text="100*100" y="0" size="18" x="0" bold="true" />
                        <e:layout>
                            <e:VerticalLayout gap="2" />
                        </e:layout>
                    </e:Group>
                </e:Group>
                <e:layout>
                    <e:VerticalLayout gap="10" />
                </e:layout>
            </e:Group>
        </e:Group>
        <e:layout>
            <e:VerticalLayout gap="10" />
        </e:layout>
    </e:Group>
</e:Skin>
