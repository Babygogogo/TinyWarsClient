                    <e:itemRendererSkinName>
                        <e:Skin width="68" height="30">
                            <e:Group id="_group" left="0" right="0" top="0" bottom="0">
                                <e:Group id="_conTileView" touchChildren="false" touchEnabled="false" left="0"
                                         bottom="4" scaleX="0.3" scaleY="0.3" width="0" height="0" />
                                <TwnsUiLabel:UiLabel id="_labelNum" text="x200" size="16" textColor="0xffffff" x="28"
                                                     strokeColor="0x000000" bold="true" bottom="4" />
                            </e:Group>
                        </e:Skin>
                    </e:itemRendererSkinName>
                    <e:layout>
                        <e:TileLayout orientation="rows" requestedColumnCount="2" verticalGap="10" />
                    </e:layout>
                </e:List>
            </TwnsUiScrollList:UiScrollList>
        </e:Group>
        <e:Group id="_groupMapInfo" right="0" bottom="70" width="180" height="180">
            <TwnsUiImage:UiImage source="commonPentagon0004" left="0" right="0" top="0" bottom="0" scale9Grid="13,3,1,1" />
            <e:Group x="10" y="10">
                <e:Group x="10" y="0">
                    <e:Group x="0" y="25">
