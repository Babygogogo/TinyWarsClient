<?xml version="1.0" encoding="utf-8"?>
<e:Skin xmlns:e="http://ns.egret.com/eui" xmlns:TwnsUiTab="TwnsUiTab.*" xmlns:TwnsUiScrollList="TwnsUiScrollList.*"
        xmlns:TwnsUiLabel="TwnsUiLabel.*" xmlns:TwnsUiImage="TwnsUiImage.*" xmlns:TwnsUiButton="TwnsUiButton.*"
        xmlns:w="http://ns.egret.com/wing" class="Skins.Component.MapInfo.MapInfo001">
    <e:Group id="_group" x="0" y="0">
        <e:Group id="_groupTile" right="0" width="180" bottom="290">
            <e:Group left="0" right="0">
                <TwnsUiImage:UiImage height="30" y="0" source="commonPentagon0005" scale9Grid="12,4,1,1" x="0"
                                     width="180" />
                <TwnsUiImage:UiImage height="30" y="10" scale9Grid="12,4,1,1" x="0" source="commonPentagon0008"
                                     width="180" />
                <TwnsUiImage:UiImage height="30" y="20" scale9Grid="12,4,1,1" x="0" source="commonPentagon0007"
                                     width="180" />
                <TwnsUiImage:UiImage height="30" y="30" scale9Grid="12,4,1,1" x="0" source="commonPentagon0006"
                                     width="180" />
                <e:layout>
                    <e:VerticalLayout gap="10" />
                </e:layout>
            </e:Group>
            <TwnsUiScrollList:UiScrollList id="_listTile" top="0" bottom="0" left="22" right="0">
