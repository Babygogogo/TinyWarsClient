        </e:Group>
        <e:Group width="760" x="222" y="16">
            <GameUi:UiScrollList id="_listWarEvent" left="0" right="0" y="0" scaleX="1" scaleY="1">
                <e:List>
                    <e:itemRendererSkinName>
                        <e:Skin currentState="up" states="down,up" width="760">
                            <e:Group left="0" right="0" top="0" bottom="0">
                                <GameUi:UiLabel id="_labelName" text="日期" size="12" textColor="0xffffff" x="10"
