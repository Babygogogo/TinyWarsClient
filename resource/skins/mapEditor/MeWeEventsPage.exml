                                        <e:HorizontalLayout/>
                                    </e:layout>
                                </e:Group>
                            </e:Group>
                        </e:Skin>
                    </e:itemRendererSkinName>
                    <e:layout>
                        <e:VerticalLayout paddingTop="4" gap="14" />
                    </e:layout>
                </e:List>
            </GameUi:UiScrollList>
            <GameUi:UiLabel id="_labelNoEvent" text="Label" y="30" horizontalCenter="0" size="24" locked="true" />
