            <GameUi:UiButton id="_btnConfirm" label="确定" y="0" x="232" skinName="Skins.Component.Button.ButtonSelfAdaption001"
                             width="120"></GameUi:UiButton>
            <e:layout>
                <e:HorizontalLayout gap="40" />
            </e:layout>
        </e:Group>
    </e:Group>
