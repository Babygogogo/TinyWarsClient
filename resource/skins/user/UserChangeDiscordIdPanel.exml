        <GameUi:UiTextInput y="210" anchorOffsetX="0" anchorOffsetY="0" maxChars="20" horizontalCenter="0.5" width="333">
            <GameUi:skinName>
                <e:Skin width="100" height="30">
                    <e:Image source="c08_t06_s01_f01" left="0" right="0" y="27" />
                    <e:EditableText id="textDisplay" size="24" height="30" textColor="0xffffff"
                                    text="https://discord.gg/jdtRpY9" left="0" right="0" textAlign="center" />
                    <e:Label id="promptDisplay" size="24" touchEnabled="false" left="0" right="0" text="" height="30"
                             textColor="0x808080" />
                </e:Skin>
            </GameUi:skinName>
        </GameUi:UiTextInput>
    </e:Group>
